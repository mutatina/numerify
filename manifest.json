{

  "manifest_version": 2,
  "name": "Numerify",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/music_note.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.chordify.net/*"],
      "js": ["numerify.js"]
    }
  ]

}